#!/usr/bin/env bash
if [ -z "$MML_ROOT" ];  then
    echo >&2 "*** error: mml-config: MML_ROOT environment variable not set"
    echo >&2 "this script should be called through matmodlab/bin/mml"
    echo >&2 "and not directly"
    exit 1
fi
PYFILE="$MML_ROOT/core/configurer.py"
$MML_PY_EXE $PYFILE "$@"
