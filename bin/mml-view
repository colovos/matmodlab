#!/usr/bin/env bash
if [ -z "$MML_ROOT" ];  then
    echo >&2 "*** ERROR: mml-view: MML_ROOT ENVIRONMENT VARIABLE NOT SET"
    echo >&2 "THIS SCRIPT SHOULD BE CALLED THROUGH matmodlab/bin/mml"
    echo >&2 "AND NOT DIRECTLY"
    exit 1
fi
PYFILE="$MML_ROOT/core/plotter.py"
PYEXE=$(which python)
$PYEXE $PYFILE "$@"
