\newcommand{\Tensor}[4][]{%
  % Main tensor typesetting macro
  % Usage: \Tensor[oper]{L}{sub}{sup}
  % Parameters
  % ----------
  % oper, #1 : Optional, prefix.  For example, specfying \dot will put
  %            a dot over the symbol L.
  % #2 : Required, the actual symbol
  % #3 : Required, subscript.  Leave empty for no subscript.
  % #4 : Required, superscript.  Leave empty for no superscript.
  \ifthenelse{\equal{#4}{}}{%
    \ifthenelse{\equal{#3}{}}{%
      % no superscript, no subscript
      {#1{\boldsymbol{#2}}}%
    }{% no superscript, subscript
      {#1{\boldsymbol{#2}}\mathsub{#3}}%
    }}{\ifthenelse{\equal{#3}{}}{%
      % superscript, no subscript
      {#1{\boldsymbol{#2}}\mathsup{#4}}%
    }{% superscript and subscript
      {#1{\boldsymbol{#2}}\mathsup{#4}\mathsub{#3}}%
    }%
  }%
}

