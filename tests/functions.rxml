<rtest name="gmd/functions">

  <keywords> builtin fast elastic function </keywords>

  <link_files>
    base.gmddiff {NAME}.rxml {NAME}.xml {NAME}.base_exo
  </link_files>

  <execute name="gmd"> {NAME}.xml </execute>

  <execute name="gmddiff">
    -f base.gmddiff {NAME}.exo {NAME}.base_exo
  </execute>

</rtest>
