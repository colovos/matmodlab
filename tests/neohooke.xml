<MMLSpec>
  <!-- {E=500} -->
  <!-- {NU=.45} -->
  <Physics driver="solid" termination_time="{1.8 * pi}">
    <Function id="2" type="analytic_expression" var="t">
      sin(t)
    </Function>
    <Function id="3" type="piecewise_linear">
      0. 2.
      1. 3.
      2. 4.
    </Function>
    <Path type="prdef" kappa="0" tstar="1" amplitude="1" format="fcnspec"
	  cfmt="222" nfac="200">
      <!-- termination time, number of steps, control, Cij -->
      {2 * pi} 2:1.e-1 0 0
    </Path>

    <Material model="umat" constants="2"
	      source="linalg.f90 neohooke.f90 umat.f90"
	      source_directory="../materials/library/neohooke">
      <Constants> {E} {NU} </Constants>
    </Material>

    <Extract format="ascii" step="1" ffmt="12.6E">
      <Variables>
	stress strain
      </Variables>
    </Extract>

  </Physics>

</MMLSpec>
