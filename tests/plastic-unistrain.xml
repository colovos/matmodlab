    <Material model="vonmises">
      <K> {K} </K>
      <G> {G} </G>
      <Y0> {Y0} </Y0>
      <H> 1.0e5 </H>
      <beta> 0.5 </beta>
    </Material>

    <Extract format="ascii" step="1" ffmt="12.6E">
      stress strain
    </Extract>

  </Physics>

</MMLSpec>
