<MMLSpec>

  <!--
       K = {K = 9.980040E+09}
       G = {G = 3.750938E+09}

       Diagnostics:
       LAM = {LAM = K - 2.0 / 3.0 * G}
       E   = {E   = 9.0 * K * G / (3.0 * K + G)}
       NU  = {NU  = (3.0 * K - 2.0 * G) / (2.0 * (3.0 * K + G))}
  -->

  <Physics>

    <Path type="prdef" kappa="0" tstar="1" estar="-.5"
	  amplitude="1" nfac="1000" ratfac="1" ndumps="20">
      <!-- termination time, number of steps, control, Cij -->
      0 0 222222 0 0 0 0 0 0
      1 1 222222 1 1 0 0 0 0
      2 1 222222 2 2 0 0 0 0
      3 1 222222 1 1 0 0 0 0
      4 1 222222 0 0 0 0 0 0
    </Path>

    <Material model="elastic">
      <K> {K} </K>
      <G> {G} </G>
    </Material>

    <Extract format="ascii" step="1" ffmt="12.6E">
      stress strain
    </Extract>

  </Physics>

</MMLSpec>
